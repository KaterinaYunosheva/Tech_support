{
  "name": "tech_support",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "yarn vite",
    "dev-yarn": "yarn vite --host",
    "build": "tsc && yarn vite build",
    "lint": "tsc --noEmit && yarn eslint . --ext .ts,.tsx --max-warnings=0 && yarn prettier --check .",
    "preview": "yarn vite preview",
    "lint:type": "tsc --noEmit",
    "lint:eslint": "yarn eslint . --ext .ts,.tsx --max-warnings=0",
    "lint:prettier": "yarn prettier --check .",
    "prettier": "yarn prettier --write ."
  },
  "dependencies": {
    "@types/node": "^20.14.10",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/config-array": "^0.17.0",
    "@eslint/js": "^9.7.0",
    "@eslint/object-schema": "^2.1.4",
    "@rollup/plugin-eslint": "^9.0.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@typescript-eslint/type-utils": "^7.16.0",
    "@typescript-eslint/utils": "^7.16.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.7.0",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^15.8.0",
    "prettier": "3.3.3",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.16.0",
    "vite": "^5.3.1",
    "vite-plugin-eslint": "^1.8.1"
  }
}
